root = true
[*]
end_of_line = lf
charset = utf-8
indent_style = space
trim_trailing_whitespace = true
insert_final_newline = true

[*.py]
charset = utf-8
insert_final_newline = true
indent_style = space
indent_size = 4

[*.yml]
indent_size = 2
indent_style = space

[*.json]
indent_size = 2
insert_final_newline = true
